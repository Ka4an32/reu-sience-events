{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "linebreak-style": 0,
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": 0,
    "max-len": 0,
    "object-curly-spacing": 0
  }
}